.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}

.index_swiper swiper image {
  width: 100%;

}

.index_cate {
  display: flex;
}

.index_cate navigator {
  flex: 1;
  padding: 20rpx;
}

.index_cate navigator image {
  width: 100%;
}


.index_floor {}

.index_floor .floor_group {}

.index_floor .floor_group .floor_title {
  padding: 10rpx 0;
}

.index_floor .floor_group .floor_title image {
  width: 100%
}

.index_floor .floor_list {
  overflow: hidden;
}

.index_floor .floor_list navigator {
  float: left;
  width: 32vw;
}

.index_floor .floor_list navigator:nth-of-type(1),
.index_floor .floor_list navigator:nth-of-type(1) image {
  height: 420rpx;
}

.index_floor .floor_list navigator:nth-last-of-type(-n+4),
.index_floor .floor_list navigator:nth-last-of-type(-n+4) image {
  height: 210rpx;
}

.index_floor .floor_list navigator:nth-last-of-type(-n+4) {
  padding-left: 1vw;
}

.index_floor .floor_list navigator:nth-of-type(2),
.index_floor .floor_list navigator:nth-of-type(2) image,
.index_floor .floor_list navigator:nth-of-type(3),
.index_floor .floor_list navigator:nth-of-type(3) image {
  height: 205rpx;
  margin-bottom: 5rpx;
}